{
  "extends": [
    "eslint-config-airbnb",
    "plugin:vue/essential"
  ],
  "rules": {
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "no-new": "off",
      "no-underscore-dangle": "off"
  },
  "env": {
      "browser": true,
      "node": true,
      "meteor": true
  }
}
